{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "eth-utils",
  "title": "Eth Utils",
  "description": "Handy utility functions and tools for Ethereum users.",
  "icon": "command-icon.png",
  "author": "Dhaiwat10",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Unit Converter",
      "description": "Convert between different units",
      "mode": "view"
    },
    {
      "name": "checksum",
      "title": "Checksum Address",
      "description": "Convert an address to checksummed address",
      "mode": "view"
    },
    {
      "name": "keccak256",
      "title": "Keccak-256 Converter",
      "description": "Keccak-256 online hash function.",
      "mode": "view"
    },
    {
      "name": "hexstring",
      "title": "Hexstring Converter",
      "description": "Hexadecimal to decimal converter.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.3",
    "bignumber.js": "^9.1.0",
    "ethers": "^5.7.1"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
